<xs:schema version="1.0"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           elementFormDefault="qualified">

    <xs:element name="SquidRatiosModel" type="SquidRatiosModel"/>
    <xs:complexType name="SquidRatiosModel" mixed="true">
        <xs:sequence>
            <xs:element name="numerator" type="SquidSpeciesModel"/>
            <xs:element name="denominator" type="SquidSpeciesModel"/>
            <xs:element name="reportingOrderIndex" type="xs:integer/>
            
            <xs:element name="ratEqTimes" minOccurs="0"/>
            <xs:complexType>
                <xs:sequence minOccurs="0" maxOccurs"unbounded">
                    <xs:element name="ratEqTime" type="xs:double"/>
                </xs:sequence>
            </xs:complexType>
            </xs:element>
            
            <xs:element name="ratEqVals" minOccurs="0"/>
            <xs:complexType>
                <xs:sequence minOccurs="0" maxOccurs"unbounded">
                    <xs:element name="ratEqVal" type="xs:double"/>
                </xs:sequence>
            </xs:complexType>
            </xs:element>
            
            <xs:element name="ratEqErrs" minOccurs="0"/>
            <xs:complexType>
                <xs:sequence minOccurs="0" maxOccurs"unbounded">
                    <xs:element name="ratEqErr" type="xs:double"/>
                </xs:sequence>
            </xs:complexType>
            </xs:element>
            
            <xs:element name="ratioVal" type="xs:double"/>
            <xs:element name="ratioFractErr" type="xs:double"/>
            <xs:element name="minIndex" type="xs:integer"/>
            <xs:element name="active" type="xs:boolean"/>
            
        </xs:sequence>
    </xs:complexType>
</xs:schema>
