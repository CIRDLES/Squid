<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
           attributeFormDefault="unqualified" 
           elementFormDefault="qualified" 
           targetNamespace="https://raw.githubusercontent.com"
           xmlns="https://raw.githubusercontent.com">
    
    <xs:complexType name="MassStationDetail" mixed="true">
        <xs:sequence>
            <xs:element name="massSationIndex" type="xs:integer"/>
            <xs:element name="massStationLabel" type="xs:string"/>
            <xs:element name="elementLabel" type="xs:string"/>
            <xs:element name="isotopeLabel" type="xs:string"/>
            <xs:element name="taskIsotopeLabel" type="xs:string"/>
            <xs:element name="isBackground" type="xs:boolean"/>
            <xs:element name="centeringTimeSec" type="xs:double"/>
            <xs:element name="uThBearingName" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
</xs:schema>
