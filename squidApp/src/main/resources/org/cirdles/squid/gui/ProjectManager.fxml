<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane fx:id="mainProjectManagerPane" prefHeight="640.0" prefWidth="925.0" styleClass="backgroundCalamari" stylesheets="@css/projectManager.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.cirdles.squid.gui.ProjectManagerController">
   <children>
      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="640.0" prefWidth="925.0">
         <children>
            <Label layoutX="9.0" layoutY="11.0" text="Project Name:" />
            <TextField fx:id="projectNameText" layoutX="119.0" layoutY="9.0" prefHeight="27.0" prefWidth="323.0" promptText="Type project name" />
            <Label layoutX="473.0" layoutY="11.0" prefHeight="20.0" prefWidth="118.0" text="Analyst Name:" />
            <TextField fx:id="analystNameText" layoutX="594.0" layoutY="8.0" prefHeight="27.0" prefWidth="322.0" promptText="Your name" />
            <Label layoutX="9.0" layoutY="48.0" text="Prawn XML file:" />
            <TextField fx:id="orignalPrawnFileName" layoutX="134.0" layoutY="43.0" prefHeight="27.0" prefWidth="783.0" promptText="name of selected Prawn XML file will appear here">
               <font>
                  <Font name="Lucida Sans Regular" size="12.0" />
               </font></TextField>
            <Label fx:id="softwareVersionLabel" layoutX="30.0" layoutY="153.0" prefHeight="20.0" prefWidth="824.0" text="Software Version:" />
            <Label fx:id="summaryStatsLabel" alignment="TOP_LEFT" layoutX="30.0" layoutY="180.0" prefHeight="99.0" prefWidth="824.0" text="Summary of analyses, duplicates, species, spots" />
            <Label fx:id="totalAnalysisTimeLabel" layoutX="30.0" layoutY="285.0" prefHeight="20.0" prefWidth="824.0" text="Total analysis time in hours" />
            <TextArea fx:id="projectNotesText" layoutX="40.0" layoutY="353.0" prefHeight="200.0" prefWidth="818.0" promptText="Notes:" />
         </children>
      </AnchorPane>
   </children>
</Pane>
