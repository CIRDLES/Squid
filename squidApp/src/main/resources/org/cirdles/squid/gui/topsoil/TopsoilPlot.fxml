<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<SplitPane dividerPositions="0.5" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.cirdles.squid.gui.topsoil.TopsoilPlotController">
   <items>
      <ListView fx:id="fractionsListView" prefHeight="200.0" prefWidth="200.0" />
      <VBox fx:id="vboxMaster" minHeight="640.0" minWidth="1000.0" stylesheets="@../css/projectManager.css">
         <children>
            <AnchorPane fx:id="plotAndConfig" VBox.vgrow="ALWAYS" />
            <ToolBar fx:id="plotToolBar" prefHeight="40.0" style="-fx-background-color: #67ccff #67ccff;" />
         </children>
      </VBox>
   </items>
</SplitPane>
